@model Booker.WebUI.Models.SelectUserModel
@{
    ViewBag.Title = "Select account";
    ViewBag.MenuSection = "admin-menu";
}
<div class="main-content">
    <div class="container">
        <div class="row">
            <div class="col-xs-12">
                <h2 class="tablesPage">@ViewBag.Title</h2>
                @using (Html.BeginForm("ChangeAccount", "Account", FormMethod.Get))
                {
                    @Html.ValidationSummary()

                    @Html.Partial("_SelectUserPartial", Model)
                }
            </div>
        </div>
    </div>
</div>
@section scripts
{
    @Scripts.Render("~/bundles/jquerydatatables")
}
